{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "Mapa": {
      "type": "object",
      "patternProperties": {
        "^\\d+$": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "Largada",
              "Rocoso",
              "Tierra",
              "Pasarela",
              "Meta"
            ]
          }
        }
      }
    }
  },
  "required": ["Mapa"]
}